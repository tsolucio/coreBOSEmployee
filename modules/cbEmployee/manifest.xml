<tables>
<table>
<name>vtiger_cbemployee</name>
<sql><![CDATA[CREATE TABLE `vtiger_cbemployee` (
  `cbemployeeid` int(11) NOT NULL,
  `cbemployeeno` varchar(50) DEFAULT NULL,
  `nombre` varchar(150) default NULL,
  `nss` varchar(100) default NULL,
  `nif` varchar(100) default NULL,
  `birthplace` varchar(100) default NULL,
  `birthday` date default NULL,
  `maritalstatus` varchar(100) default NULL,
  `numchildren` int(11) default NULL,
  `inchargeof` int(11) default NULL,
  `userid` int(11) default NULL,
  `age` int(11) default NULL,
  `situacion` varchar(150) default NULL,
  `categoria` varchar(150) default NULL,
  `yearly_compensation` varchar(255) DEFAULT NULL,
  `tipocontrato` varchar(150) default NULL,
  `hourly_compensation` varchar(255) DEFAULT NULL,
  `departamento` varchar(150) default NULL,
  `altaempresa` date default NULL,
  `street_address` varchar(200) DEFAULT NULL,
  `city` varchar(100) DEFAULT NULL,
  `state` varchar(100) DEFAULT NULL,
  `zip_postal_code` varchar(100) DEFAULT NULL,
  `home_phone` varchar(20) DEFAULT NULL,
  `mobile_phone` varchar(20) DEFAULT NULL,
  `work_phone` varchar(20) DEFAULT NULL,
  `personal_email` varchar(100) DEFAULT NULL,
  `work_email` varchar(100) DEFAULT NULL,
  `linkedin_page` varchar(150) DEFAULT NULL,
  `twitter_page` varchar(150) DEFAULT NULL,
  `facebook_page` varchar(150) DEFAULT NULL,
  `skypeid` varchar(150) DEFAULT NULL,
  `gtalkid` varchar(150) DEFAULT NULL,
  `high_school` varchar(255) DEFAULT NULL,
  `h_s_graduation_year` varchar(100) DEFAULT NULL,
  `high_school_gpa` varchar(255) DEFAULT NULL,
  `h_s_comments` varchar(255) DEFAULT NULL,
  `undergraduate_school` varchar(255) DEFAULT NULL,
  `u_s_graduation_year` varchar(100) DEFAULT NULL,
  `u_s_gpa` varchar(255) DEFAULT NULL,
  `u_s_comments` varchar(255) DEFAULT NULL,
  `post_grad_school` varchar(255) DEFAULT NULL,
  `p_g_graduation_year` varchar(100) DEFAULT NULL,
  `p_g_gpa` varchar(255) DEFAULT NULL,
  `p_g_comments` varchar(255) DEFAULT NULL,
  PRIMARY KEY (`cbemployeeid`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8]]></sql>
</table>
